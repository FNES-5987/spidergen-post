            <input data-base="ATextField" data-class="ATextField" type="password" value="" class="ATextField-Style" autocomplete="off" style="width: 170px; height: 20px; position: relative; margin-left: 10px; background-color: rgba(0, 0, 0, 0);" id="create_new_post--user_password" placeholder="비밀번호" data-blocks-ac001="InBlock1,String">
        </div>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: auto; position: relative; left: 0px; margin-top: 10px; text-align: center; background-color: rgba(0, 0, 0, 0);" id="create_new_post--writing">
            <input data-base="ATextField" data-class="ATextField" type="text" value="" class="ATextField-Style" autocomplete="off" style="width: 90%; height: 50px; position: relative; text-align: left; font-size: 20px; background-color: rgba(0, 0, 0, 0); padding-left: 10px;" placeholder="제목을 입력하세요" id="create_new_post--writing_title" data-blocks-ac001="InBlock1,String">
