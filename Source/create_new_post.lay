<div data-base="AView" data-class="create_new_post" class="AView-Style" style="left: 0px; top: 0px; width: 720px; height: auto; background-color: rgb(220, 220, 220); position: absolute;" data-browser-width="720" data-class-map="{&quot;afc&quot;:[&quot;AView&quot;,&quot;ALabel&quot;,&quot;ATextField&quot;,&quot;ATextArea&quot;,&quot;AButton&quot;]}">
    <div data-base="AView" data-class="AView" class="AView-Style" style="width: 100%; height: auto; position: relative; left: 0px; top: 0px; padding: 25px; border-width: 1px; border-style: solid; border-color: rgb(0, 0, 0); background-color: rgb(255, 255, 255);" id="create_new_post--post_body">
        <label data-base="ALabel" data-class="ALabel" class="ALabel-Style" style="width: auto; height: auto; position: relative; font-size: 20px; font-weight: bold;">포스트 생성</label>
        <div data-base="AView" data-class="AView" class="AView-Style" style="width: 90%; height: auto; position: relative; left: 0px; background-color: rgba(0, 0, 0, 0); margin-top: 10px; margin-left: auto; margin-right: auto;" id="create_new_post--user-info">
